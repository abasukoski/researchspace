}""" .

    <http://www.researchspace.org/bnode/n4bf215c67ab24123a56a8e566a1afad0b2> a ns1:Query ;
        ns1:text """SELECT DISTINCT ?value ?label WHERE {
	$subject crm:P48_has_preferred_identifier $value . 
      
