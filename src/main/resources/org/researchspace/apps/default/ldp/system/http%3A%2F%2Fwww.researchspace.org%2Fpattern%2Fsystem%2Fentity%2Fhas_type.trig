    <http://www.researchspace.org/bnode/n0c565a4ada1546bdb19039f6552b6712b1> a ns2:Query ;
        ns2:text """SELECT DISTINCT ?value ?label WHERE {
  ?value a ?ontologyClass .
  ?ontologyClass rdfs:subClassOf* crm:E55_Type .
  MINUS { 
      ?value crm:P71i_is_listed_in|skos:inScheme ?authorityList . 
      ?authorityList crm:P2_has_type Platform:System_Resource .
  }
  ?value skos:prefLabel ?label .
} ORDER BY ASC(LCASE(STR(?label)))""" .

    <http://www.researchspace.org/bnode/n0c565a4ada1546bdb19039f6552b6712b2> a ns2:Query ;
        ns2:text """INSERT { 
  $subject crm:P2_has_type $value .
  $value crm:P2i_is_type_of $subject .
} WHERE {}""" .

    <http://www.researchspace.org/bnode/n0c565a4ada1546bdb19039f6552b6712b3> a ns2:Query ;
        ns2:text """SELECT DISTINCT ?value ?label WHERE {
  $subject crm:P2_has_type $value .
      
  MINUS { 
      #types from System authorities are removed from the selection
      ?value crm:P71i_is_listed_in|skos:inScheme ?authorityList . 
      ?authorityList crm:P2_has_type Platform:System_Resource .
  }
  ?value a ?ontologyClass .
  ?ontologyClass rdfs:subClassOf* crm:E55_Type .
  ?value skos:prefLabel ?label .
}""" .

    <http://www.researchspace.org/bnode/n0c565a4ada1546bdb19039f6552b6712b4> a ns2:Query ;
        ns2:text """DELETE { 
  $subject crm:P2_has_type $value .
  $value crm:P2i_is_type_of $subject .
} WHERE { 
  $subject crm:P2_has_type $value .
  $value crm:P2i_is_type_of $subject .

  MINUS { 
      ?value crm:P71i_is_listed_in|skos:inScheme ?authorityList . 
      ?authorityList crm:P2_has_type Platform:System_Resource .
  }
  ?value a ?ontologyClass .
  ?ontologyClass rdfs:subClassOf* crm:E55_Type .
  ?value skos:prefLabel ?label .
}""" .

    <http://www.researchspace.org/bnode/n0c565a4ada1546bdb19039f6552b6712b5> a ns2:Query ;
        ns2:text """SELECT DISTINCT ?value ?label WHERE {
  ?value a ?ontologyClass .
  ?ontologyClass rdfs:subClassOf* crm:E55_Type .
  MINUS { 
      ?value crm:P71i_is_listed_in|skos:inScheme ?authorityList . 
      ?authorityList crm:P2_has_type Platform:System_Resource .
  }
  ?value skos:prefLabel ?label .
  MINUS { ?value crm:P71i_is_listed_in|skos:inScheme ?systemAuthority . ?systemAuthority crm:P2_has_type Platform:System_Resource . }
  FILTER REGEX(LCASE(STR(?label)), "?token", "i")
} ORDER BY ASC(?label)
  LIMIT 10""" .

