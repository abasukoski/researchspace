    <http://www.researchspace.org/bnode/nd16979e4fcbb480e9b99a722393460c7b1> a ns1:Query ;
        ns1:text """DELETE { 
    $subject crm:P02_has_range $value .
    $value crm:P02i_is_range_of $subject .
} WHERE {
     $subject crm:P02_has_range $value .
     $value crm:P02i_is_range_of $subject .
      
    ?value a ?ontologyClass .
    ?ontologyClass rdfs:subClassOf* crm:E55_Type .
    ?value skos:prefLabel ?label .
}""" .

    <http://www.researchspace.org/bnode/nd16979e4fcbb480e9b99a722393460c7b2> a ns1:Query ;
        ns1:text """SELECT DISTINCT ?value ?label WHERE {
  	$subject crm:P02_has_range ?value .
      
    ?value a ?ontologyClass .
    ?ontologyClass rdfs:subClassOf* crm:E55_Type .
    ?value skos:prefLabel ?label .
}""" .

    <http://www.researchspace.org/bnode/nd16979e4fcbb480e9b99a722393460c7b3> a ns1:Query ;
        ns1:text """SELECT DISTINCT ?value ?label WHERE {
    ?value a ?ontologyClass .
    ?ontologyClass rdfs:subClassOf* crm:E55_Type .
    ?value skos:prefLabel ?label .
  MINUS { ?value crm:P71i_is_listed_in|skos:inScheme ?systemAuthority . ?systemAuthority crm:P2_has_type Platform:System_Resource . }
  FILTER REGEX(LCASE(STR(?label)), "?token", "i")
} ORDER BY ASC(?label) 
  LIMIT 10 """ .

    <http://www.researchspace.org/bnode/nd16979e4fcbb480e9b99a722393460c7b4> a ns1:Query ;
