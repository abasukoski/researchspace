  ?digitizationProcess crmdig:L11_had_output $value . 
} WHERE { 
  $subject crmdig:L60i_is_documented_by ?digitizationProcess .
  ?digitizationProcess a crmdig:D2_Digitization_Process .    
  ?digitizationProcess crmdig:L11_had_output $value . 
      
  ?value a rs:EX_File .
}""" .

    <http://www.researchspace.org/bnode/n379ffe421aa44f3eb8b3914fdf98fd58b2> a ns3:Query ;
        ns3:text """INSERT { 
  $subject crmdig:L60i_is_documented_by ?digitizationProcess .
  ?digitizationProcess a crmdig:D2_Digitization_Process .    
  ?digitizationProcess crmdig:L11_had_output $value . 
} WHERE {
	BIND(URI(CONCAT(str($subject),"/digitization_process")) as ?digitizationProcess)
}""" .

    <http://www.researchspace.org/bnode/n379ffe421aa44f3eb8b3914fdf98fd58b3> a ns3:Query ;
        ns3:text """SELECT DISTINCT ?value ?label WHERE {
